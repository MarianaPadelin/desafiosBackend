<div>
  <h2>Lista de productos</h2>

  <table>
    <tr>
      <th>ID</th>
      <th>TITLE</th>
      <th>DESCRIPTION</th>
      <th>PRICE</th>
      <th>CODE</th>
      <th>STATUS</th>
      <th>CATEGORY</th>
      <th>STOCK</th>
      <th>THUMBNAILS</th>
    </tr>
    <tbody>
   {{#each products}}
    <tr>
      <td>{{this.id}}</td>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.price}}</td>
      <td>{{this.code}}</td>
      <td>{{this.status}}</td>
      <td>{{this.category}}</td>
      <td>{{this.stock}}</td>
      <td>{{this.thumbnails}}</td>
    </tr>
 {{/each}}
    </tbody>
  </table>
<br>
<a href="/chat"><button> Ir al chat </button></a>
<a href="/api/carts"><button> Ir al carrito </button></a>


  <script src="/socket.io/socket.io.js"></script>
  <script src="/js/main.js"></script>
</div>